{
  "name": "ChristFellowship",
  "version": "6.0.0",
  "private": true,
  "engines": {
    "node": ">=12.x.x"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "android": "SIM=\"$($HOME/Library/Android/sdk/emulator/emulator -list-avds | sed -n 1p)\" && $HOME/Library/Android/sdk/emulator/emulator -avd \"${SIM}\" & react-native run-android --appId com.differential.apollos",
    "beta": "./scripts/add-packages.sh beta",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "bugsnag-release": "yarn bugsnag-release:android && yarn bugsnag-release:ios",
    "bugsnag-release:android": "yarn generate-source-map:android && yarn upload-source-map:android",
    "bugsnag-release:ios": "yarn generate-source-map:ios && yarn upload-source-map:ios",
    "canary": "./scripts/add-packages.sh canary",
    "codecov": "cat ./coverage/lcov.info | codecov",
    "fixlint": "eslint ./src --fix",
    "generate-stories": "rnstl --searchDir ./src --pattern **/*.stories.js",
    "generate-source-map:android": "bash ./scripts/generate-bugsnag-source-map.sh android",
    "generate-source-map:ios": "bash ./scripts/generate-bugsnag-source-map.sh ios",
    "get-schema": "eval $(grep ^APP_DATA_URL .env) && sleep 5 && get-graphql-schema $APP_DATA_URL > schema.graphql",
    "go": "yarn && react-native link && yarn pods && yarn start",
    "icons": "react-native set-icon --path",
    "ios": "react-native run-ios --simulator=\"iPhone 11\"",
    "latest": "./scripts/add-packages.sh latest",
    "link-packages": "node ./scripts/link-packages && yarn yalc update",
    "lint": "eslint ./src --ext .js",
    "nuke": "yarn nuke:node && yarn nuke:cache",
    "nuke:cache": "watchman watch-del-all && react-native start --reset-cache",
    "nuke:node": "rm -rdf ./node_modules && yarn cache clean && yarn",
    "pods": "(bundle install && cd ios && bundle exec pod install --repo-update)",
    "postinstall": "jetify && patch-package && bash ./scripts/get-zoom-sdk.sh && yarn pods",
    "postversion": "react-native-version --never-amend --android android/app/build.gradle --ios ios/",
    "prod": "ZOOM_SDK_STAGE=prod yarn nuke:node && yarn zoom:prod",
    "prepublish": "lerna run build && lerna run generate-stories",
    "prestart": "yarn generate-stories & node ./scripts/get-introspection-data.js",
    "setup": "./scripts/init.sh",
    "splash": "react-native set-splash --path",
    "start": "yarn start:dev",
    "start:dev": "yarn generate-stories & yarn get-schema && node ./scripts/get-introspection-data.js && react-native start",
    "storybook:disable": "sed -i '' 's/STORYBOOK=true/STORYBOOK=false/g' .env",
    "storybook:enable": "sed -i '' 's/STORYBOOK=false/STORYBOOK=true/g' .env",
    "test": "TZ=UTC jest",
    "unlink-packages": "node ./scripts/link-packages remove && rm -rf ./node_modules",
    "upgrade": "npx @apollosproject/upgrade-tools upgrade",
    "upload-source-map:android": "bash ./scripts/upload-bugsnag-source-map.sh android",
    "upload-source-map:ios": "bash ./scripts/upload-bugsnag-source-map.sh ios",
    "version": "node ../../scripts/update-apollos-version.js && git add apollos.json",
    "zoom": "patch-package && bash ./scripts/get-zoom-sdk.sh && yarn pods",
    "zoom:dev": "bash yarn zoom",
    "zoom:prod": "ZOOM_SDK_STAGE=prod yarn zoom"
  },
